# DataFrame から '地域区分' 列を作成
df2['地域区分'] = ''
# '札幌市' を含む場合は '地域区分' を '札幌市' に設定
df2.loc[df2['A市区町村'].str.contains('札幌市'), '地域区分'] = '札幌市'
# '旭川市'、'函館市'、'帯広市'、'釧路市'、'室蘭市'、'小樽市'、'苫小牧市' の場合は '地域区分' を '中核都市' に設定
core_cities = ['旭川市', '函館市', '帯広市', '釧路市', '室蘭市', '小樽市', '苫小牧市']
df2.loc[df2['A市区町村'].isin(core_cities), '地域区分'] = '中核都市'
# '地域区分' が東京都の行は '東京都' に設定
df2.loc[df2['A市区町村'] == '東京都', '地域区分'] = '東京都'
# '地域区分' が空欄の行は '地方市町村' に設定
df2.loc[df2['地域区分'] == '', '地域区分'] = '地方市町村'

